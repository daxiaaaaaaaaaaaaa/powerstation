<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--
    对layer-list样式，是按照顺序堆叠的，所以最上层的放在最后面，而且如果设置了progressDrawable
    的属性，再去设置progressbar的background是没有效果的
    -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10dp"/>         <!-- 圆角 -->

            <solid android:color="@color/color_progress_normal"/>   <!-- 描边 -->
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dp"/>
                <solid android:color="@color/color_progress_finish"/>   <!-- 描边 -->
            </shape>
        </clip>
    </item>
</layer-list>